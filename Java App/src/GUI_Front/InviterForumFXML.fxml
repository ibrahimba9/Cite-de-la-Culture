<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI_Front.InviterForumFXMLController">
   <children>
      <Text layoutX="112.0" layoutY="112.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login Abonné à inviter:" />
      <TextField fx:id="tfabonne" layoutX="250.0" layoutY="95.0" />
      <Button fx:id="btnchercher" layoutX="420.0" layoutY="95.0" mnemonicParsing="false" onAction="#chercherAbonne" text="Chercher" />
      <Text layoutX="70.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Inviter un Abonné à Rejoindre une Discussion">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <TableView fx:id="viewabonne" layoutX="116.0" layoutY="141.0" prefHeight="200.0" prefWidth="368.0">
        <columns>
          <TableColumn fx:id="clogin" prefWidth="122.0" text="Login" />
          <TableColumn fx:id="cnom" prefWidth="122.0" text="Nom" />
            <TableColumn fx:id="cprenom" prefWidth="122.0" text="Prenom" />
        </columns>
      </TableView>
      <Button fx:id="btninviter" layoutX="425.0" layoutY="356.0" mnemonicParsing="false" onAction="#inviterAbonne" text="Inviter">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnback" layoutX="37.0" layoutY="359.0" mnemonicParsing="false" onAction="#goBackToForum" text="Retour" />
   </children>
</AnchorPane>
